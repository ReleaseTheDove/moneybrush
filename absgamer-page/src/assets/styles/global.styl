*
  box-sizing  border-box
body
  min-width 1020px
  word-wrap break-word
  font-family -apple-system,BlinkMacSystemFont,"Segoe UI",Helvetica,Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol"
  font-size 14px
  line-height 1.5
  color #24292e
  margin 0
  display block
  background-color #f9f9f9
  // background-image url('../images/bg.png')
label
    font-weight: 600
input
  font-family inherit
  font-size inherit
  line-height inherit
  overflow visible
  font inherit
  margin 0
ul, ol
  padding-left  0
  margin-top 0
  margin-bottom 0
img
 border-style none
a
  color #0366d6
  text-decoration none
  background-color transparent
h3
  font-size 20px
  font-weight 600
h1, h2, h3, h4, h5, h6
  margin-top 0
  margin-bottom 0
p
  margin-top 0
  margin-bottom 10px
.list-style-none
  list-style none
.text-bold
  font-weight 600
.f5
  font-size 14px
.mt-4
  margin-top 24px
.mt-3
  margin-top 16px
.mt-2
  margin-top 8px
.columns
  margin-right -10px
  margin-left -10px
.one-fourth
  width 25%
.column
  float left
  padding-right 10px
  padding-left 10px
.border
  border-color rgb(225, 228, 232)
  border-style  solid
  border-width  1px
.menu
  margin-bottom 15px
  list-style none
  background-color #ffffff
  border-radius 3px
.menu-items:first-child
  border-top 0
  border-top-left-radius  2px
  border-top-right-radius 2px
.menu-item
  position relative
  display block
  padding 8px 10px 8px 10px
  border-bottom 1px solid rgb(225, 228, 232)
