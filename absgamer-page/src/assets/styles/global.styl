.d-flex
  display flex
.flex-auto
  flex 1 1 auto
.pb-3
  padding-bottom 16px
.flex-justify-between
  justify-content space-between
.flex-items-center
  align-items center
.border-bottom
  border-bottom 1px #e1e4e8 solid
.px-3
  padding-right 16px
  padding-left 16px
.px-2
  padding-right 8px
  padding-left 8px
.position-relative
  position relative
.pt-6
  padding-top 40px
.pb-2
  padding-bottom 8px
.mt-6
  margin-top 40px
.text-gray
  color #586069
.border-gray-light
  border-color #eaecef
.border-top
  border-top 1px  #e1e4e8 solid

.form-control, .form-select
    min-height 34px
    padding 6px 8px
    font-size 16px
    line-height 20px
    color #24292e
    vertical-align middle
    background-color #ffffff
    background-repeat no-repeat
    background-position right 8px center
    border 1px solid #d1d5da
    border-radius 3px
    outline none
    box-shadow inset 0 1px 2px rgba(27,31,35,0.075)
