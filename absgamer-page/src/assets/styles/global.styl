body
  word-wrap break-word
  font-family -apple-system,BlinkMacSystemFont,"Segoe UI",Helvetica,Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol"
  font-size 14px
  line-height 1.5
  color #24292e
  margin 0
  box-sizing border-box
  display block
  background-color #f9f9f9
  min-width 0 !important
input
  font-size inherit
  min-height 30px
  color #ffffff
  display table-cell
  width 100%
  padding-top 0
  padding-bottom 0
  background none
  border 0
  box-shadow none
  padding-left 8px
  padding-right 8px
  line-height 20px
  vertical-align  middle
  border-radius 3px
  outline none
  font-family inherit
  overflow visiable
label
  min-height 30px
  font-size inherit
  color #ffffff
  background-color rgba(255, 255, 255, 0.125)
  border 0
  box-shadow none
  width 100%
  max-width 100%
  padding 0
  font-weight 400
  vertical-align middle
  display flex
  position relative
  align-items center
  justify-content space-between
  line-height 20px
  background-repeat no-repeat
  background-position right 8px center
  border-radius 3px
  outline none
ul
  display flex
  list-style none
  font-weight 600
  padding-left 8px
  align-items center
  margin-top 0
  margin-bottom 0
a
  color #0366d6
  text-decoration none
  background-color transparent