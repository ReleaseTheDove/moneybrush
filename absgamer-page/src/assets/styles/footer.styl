#footer
  padding-right 16px
  padding-left 16px
  max-width 1012px
  margin-right auto
  margin-left auto

.footer-content
  display flex
  font-size 12px
  padding-top 40px
  padding-bottom 8px
  margin-top 40px
  position relative
  justify-content space-between
  color #586069
  border-color #eaecef
  border-top 1px #e1e4e8 solid

.footer-ul
  display flex
  list-style none
  flex-wrap wrap
  padding-left 0
  margin-top 0
  margin-bottom 0

.mr-3
  margin-right 16px

a
  background-color transparent
  color #0366d6
  text-decoration none

.footer-blank
  display flex
  padding-bottom 40px
  justify-content center
  